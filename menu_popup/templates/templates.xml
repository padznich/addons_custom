<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <template id="assets_backend" name="menu popup assets" inherit_id="web.assets_backend">
            <xpath expr="//script[last()]" position="after">
            	<script type="text/javascript" src="/menu_popup/static/lib/swipebox/jquery.swipebox.js"></script>
            	<script type="text/javascript" src="/menu_popup/static/src/js/config.js"></script>
            </xpath>
            <xpath expr="//link[last()]" position="after">
				<link rel="stylesheet" type="text/css" href="/menu_popup/static/src/css/swipebox.css" />
				<link rel="stylesheet" type="text/css" href="/menu_popup/static/src/css/base.css" />
			</xpath>
       </template>

    </data>
</openerp>
